{
  "presets": ["es2015", "react", "stage-0"],
  "plugins": [
    "add-module-exports",
    "babel-root-import",
    ["provide-modules", {
      "debug": "debug",
      "react": {
        "default": "React",
        "destructured": ["PropTypes"],
      }
    }],
    "transform-decorators-legacy"
  ]
}
